{
  "name": "loclink-cms-koa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./main.ts",
    "build": "npm run clean && tsc && cc copy ./src/app/keys ./dist/src/app/keys",
    "clean": "cc rm ./dist",
    "start": "node ./dist/main.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration": "npm run typeorm migration:generate ./src/migrations/update-table -- -d ./src/app/database.ts",
    "reload": "npm run typeorm migration:run -- -d ./src/app/database.ts",
    "init:db": "cross-env NODE_ENV=INIT ts-node ./src/app/init.ts "
  },
  "types": "index.d.ts",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "koa2-cors": "^2.0.6",
    "mysql2": "^2.3.3",
    "pm2": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-router": "^7.4.4",
    "@types/koa2-cors": "^2.0.2",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cli-cat": "^0.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
